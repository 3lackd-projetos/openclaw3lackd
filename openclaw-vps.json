{
  "meta": {
    "lastTouchedVersion": "2026.2.6-3",
    "lastTouchedAt": "2026-02-09T22:55:14.238Z"
  },
  "env": {
    "DEEPSEEK_API_KEY": "${DEEPSEEK_API_KEY}"
  },
  "auth": {
    "profiles": {}
  },
  "models": {
    "providers": {
      "deepseek": {
        "baseUrl": "https://api.deepseek.com/v1",
        "apiKey": "${DEEPSEEK_API_KEY}",
        "api": "openai-completions",
        "models": [
          {
            "id": "deepseek-reasoner",
            "name": "DeepSeek R1",
            "reasoning": true,
            "input": [
              "text"
            ],
            "cost": {
              "input": 0,
              "output": 0,
              "cacheRead": 0,
              "cacheWrite": 0
            },
            "contextWindow": 64000,
            "maxTokens": 8192
          },
          {
            "id": "deepseek-chat",
            "name": "DeepSeek V3",
            "reasoning": false,
            "input": [
              "text"
            ],
            "cost": {
              "input": 0,
              "output": 0,
              "cacheRead": 0,
              "cacheWrite": 0
            },
            "contextWindow": 64000,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "deepseek/deepseek-reasoner",
        "fallbacks": [
          "deepseek/deepseek-chat"
        ]
      },
      "models": {
        "deepseek/deepseek-reasoner": {
          "alias": "DeepSeek R1"
        },
        "deepseek/deepseek-chat": {
          "alias": "DeepSeek V3"
        },
        "ollama/llama3.1:latest": {
          "alias": "Llama3.1 Latest"
        },
        "ollama/openhermes:latest": {
          "alias": "Openhermes Latest"
        },
        "ollama/codellama:13b-instruct-q5_K_M": {
          "alias": "Codellama 13B Instruct Q5_K_M"
        },
        "ollama/mixtral:8x7b-instruct-v0.1-q4_K_M": {
          "alias": "Mixtral 8X7B Instruct V0.1 Q4_K_M"
        },
        "ollama/llama2:13b-chat-q5_K_M": {
          "alias": "Llama2 13B Chat Q5_K_M"
        },
        "ollama/qwen2.5-coder:32b": {
          "alias": "Qwen2.5 Coder 32B"
        },
        "ollama/command-r:latest": {
          "alias": "Command R Latest"
        }
      },
      "workspace": "/root/.openclaw/workspace",
      "compaction": {
        "mode": "safeguard",
        "reserveTokensFloor": 30000,
        "memoryFlush": {
          "enabled": true,
          "softThresholdTokens": 8000,
          "prompt": "Write any lasting notes to memory/YYYY-MM-DD.md; reply with NO_REPLY if nothing to store.",
          "systemPrompt": "Session nearing compaction. Store durable memories now."
        }
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      }
    },
    "list": [
      {
        "id": "dev",
        "name": "Developer",
        "model": {
          "primary": "openai/gpt-5.1-codex",
          "fallbacks": [
            "deepseek/deepseek-chat"
          ]
        },
        "tools": {
          "allow": [
            "read",
            "write",
            "exec",
            "list_dir",
            "view_file",
            "replace_file_content",
            "multi_replace_file_content",
            "find_by_name",
            "grep_search"
          ]
        },
        "identity": {
          "name": "Developer"
        }
      },
      {
        "id": "researcher",
        "name": "Researcher",
        "model": {
          "primary": "deepseek/deepseek-reasoner",
          "fallbacks": [
            "deepseek/deepseek-chat"
          ]
        },
        "tools": {
          "allow": [
            "web_search",
            "web_fetch"
          ]
        },
        "identity": {
          "name": "Researcher"
        }
      }
    ]
  },
  "tools": {
    "web": {
      "search": {
        "apiKey": "${WEB_SEARCH_API_KEY}"
      }
    }
  },
  "messages": {
    "ackReactionScope": "group-mentions"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto",
    "restart": true
  },
  "channels": {
    "whatsapp": {
      "dmPolicy": "allowlist",
      "selfChatMode": false,
      "allowFrom": [
        "+5511963972187"
      ],
      "groupPolicy": "allowlist",
      "mediaMaxMb": 50,
      "debounceMs": 0
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "loopback",
    "auth": {
      "mode": "token",
      "token": "${GATEWAY_TOKEN}"
    },
    "tailscale": {
      "mode": "off",
      "resetOnExit": false
    }
  },
  "skills": {
    "install": {
      "nodeManager": "npm"
    }
  },
  "plugins": {
    "entries": {
      "whatsapp": {
        "enabled": true
      },
      "google-antigravity-auth": {
        "enabled": false
      }
    }
  }
}
